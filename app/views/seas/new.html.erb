<div class="container">
<%= form_for(@sea) do |f| %>
  <label>Name:</label>
  <%= f.text_field :name %><br>

  <label>Bio:</label>
  <%= f.text_field :bio %><br>
  
  <label>Image URL:</label>
  <%= f.text_field :image_url %><br>
  
  <label>Temperature:</label>
  <%= f.text_field :temperature %><br>
  
  <label>Favorite Color:</label>
  <%= f.text_field :favorite_color %><br>
  
  <label>Scariest Creature:</label>
  <%= f.text_field :scariest_creature %><br>
  
  <label>Mood:</label>
  <%= f.text_field :mood %><br>

  <label>has mermaids</label>
  <%= f.check_box (:has_mermaids)%>



  <%= f.submit("Submit")%>
  <% end %>
  </div>
  